class Solution {
public:
    int fib(int n) {
        int a=0;
        int b=1;
        int count=2;
        int c;
        if(n==0 || n==1){
            return n;
        }
        while(count<=n){
            c=a+b;
            a=b;
            b=c;
            count++;

        }
        return c;

    }
};
